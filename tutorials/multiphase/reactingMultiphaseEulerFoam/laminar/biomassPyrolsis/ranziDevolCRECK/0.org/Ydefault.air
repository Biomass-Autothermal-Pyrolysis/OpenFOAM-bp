FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Ydefault.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    gasInlet
    {
        type                fixedValue;
        value               $internalField;
    }
    biomassInlet
    {
        type                fixedValue;
        value               $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    walls
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            empty;
    }
}

