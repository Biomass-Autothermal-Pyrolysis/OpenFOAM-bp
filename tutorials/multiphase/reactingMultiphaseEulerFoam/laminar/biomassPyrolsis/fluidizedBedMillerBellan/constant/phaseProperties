FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type basicMultiphaseSystem;

phases (air particles1 particles2);

air
{
    type          reactingPhaseModel;

    diameterModel constant;
    constantCoeffs
    {
        d               1;
    }
    Sct 0.7;
    residualAlpha   1e-4;
}

particles1
{
    type          purePhaseModel;

    diameterModel constant;
    constantCoeffs
    {
        d               6e-4;
    }

    alphaMax        0.63;
    residualAlpha   1e-4;
}

particles2
{
    type            reactingPhaseModel;

    diameterModel constant;
    constantCoeffs
    {
        d               15.87e-4;
    }

    alphaMax        0.63;
    residualAlpha   1e-4;
    Sct 0.7;
}

blending
{

    default
    {
        type                none;
        continuousPhase     air;
        minFullyContinuousAlpha.air 0;
        minPartlyContinuousAlpha.air 0;
        minFullyContinuousAlpha.particles1 1.0;
        minPartlyContinuousAlpha.particles1 1.0;
        minFullyContinuousAlpha.particles2 1.0;
        minPartlyContinuousAlpha.particles2 1.0;
    }
}

interfaceCompression
(
);

interfaceComposition
(
);

surfaceTension
(
    (air and particles1)
    {
        type            constant;
        sigma           0;
    }
    (air and particles2)
    {
        type            constant;
        sigma           0;
    }
    (particles1 and particles2)
    {
        type            constant;
        sigma           0;
    }
);


aspectRatio
(
);

drag
(
    (particles1 in air)
    {
        type            GidaspowErgunWenYu;
        residualRe      1e-3;
        swarmCorrection
        {
            type        none;
        }
    }
    (particles2 in air)
    {
        type            GidaspowErgunWenYu;
        residualRe      1e-3;
        swarmCorrection
        {
            type        none;
        }
    }
    (particles1 and particles2)
    {
        type            Syamlal;
        residualRe      1e-3;
        swarmCorrection
        {
            type        none;
        }
    }
);

virtualMass
(
);

heatTransfer
(
    (particles1 in air)
    {
        type            Gunn;
        residualAlpha   1e-4;
    }

    (particles2 in air)
    {
        type            Gunn;
        residualAlpha   1e-4;
    }
    (particles2 in particles1)
    {
        type            spherical;
        residualAlpha   1e-4;
    }
);

lift
(
);

wallLubrication
(
);

turbulentDispersion
(
);

phaseTransfer
(
    (particles2 in air)
    {
        type reactionDriven;
        reactingPhase particles2;
        targetPhase air;
        species (gas tar);
    }
);

kineticTheory
{
    name                    Particles;
    alphaMax                0.63;
    residualAlpha           1e-4;

    packingLimitModel       YuStandish;
    radialModel             Lebowitz;
    viscosityModel          Princeton;
    conductivityModel       Princeton;
    granularPressureModel   Princeton;
    frictionalStressModel   Princeton;

    coeffRest
    (
        (particles1 and particles1) 0.8
        (particles1 and particles2) 0.8
        (particles2 and particles2) 0.6
    );
    coeffFric
    (
        (particles1 and particles1) 0.1
        (particles1 and particles2) 0.1
        (particles2 and particles2) 0.1
    );

    JohnsonJacksonCoeffs
    {
        alphaMinFriction        0.61;
        Fr                      0.05;
        eta                     2;
        p                       5;
        phi                     28.5;
        alphaDeltaMin           1e-3;
    }

    PrincetonCoeffs
    {
	alphaMinFrictionByAlphap 0.97;
	alphaMinFriction	0.55;
	Fr			0.05;
	eta			2;
	p			5;
	phi			28.5;
	alphaDeltaMin		0.001;
    }
}

// Minimum allowable pressure
pMin            10000;


