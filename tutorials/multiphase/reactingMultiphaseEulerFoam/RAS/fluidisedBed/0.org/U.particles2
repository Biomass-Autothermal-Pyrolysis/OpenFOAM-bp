FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U.particles2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type               fixedValue;
        value              uniform (0 0 0);
    }

    outlet
    {
        type               fixedValue;
        value              uniform (0 0 0);
    }

    walls
    {
        type                     JohnsonJacksonParticleSlip;
        restitutionCoefficient   0.2;
        specularityCoefficient   0.1;
        muF                      0.25;
        sigma                    2;
        value                    uniform (0 0 0);
    }

    defaultFaces
    {
        type               empty;
    }
}

